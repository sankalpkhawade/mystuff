<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="employee" class="com.cts.core.Employee1">
		<constructor-arg name="employeeId" type="int" index="0"
			value="1122"></constructor-arg>
		<constructor-arg name="employeeName" type="String"
			index="1" value="Rod Johnson"></constructor-arg>
		<constructor-arg name="username" type="String" index="2"
			value="Rod"></constructor-arg>
		<constructor-arg name="password" type="String" index="3"
			value="spring"></constructor-arg>
	</bean>

</beans>
 